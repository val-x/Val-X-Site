{
  "npm.packageManager": "bun",
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "editor.defaultFormatter": "vscode.typescript-language-features",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.js": "javascript"
  },
  "git.flow.enabled": true,
  "git.flow.defaultBranchName.develop": "develop",
  "git.flow.defaultBranchName.feature": "feature/",
  "git.flow.defaultBranchName.release": "release/",
  "git.flow.defaultBranchName.hotfix": "hotfix/",
  "git.defaultBranch": "develop"
}
